{
  "presets": ["@babel/env", "@babel/preset-react"],
  "plugins": [
    ["@babel/plugin-proposal-class-properties"],
    [
      "module-resolver",
      {
        "root": ["./"],
        "alias": {
          "components": "frontend/components",
          "ducks": "frontend/ducks",
          "controllers": "backend/controllers"
        }
      }
    ]
  ]
}
